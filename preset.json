{
    "useConfigFiles": true,
    "cssPreprocessor": "sass",
    "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-eslint": {
          "config": "standard",
          "lintOn": ["save"]
        },
        "vue-cli-plugin-vuetify": {
            "preset": "configure",
            "replaceComponents": false,
            "useTheme": false,
            "useCustomProperties": false,
            "iconFont": "mdi",
            "installFonts": false,
            "useAlaCarte": true,
            "usePolyfill": true,
            "locale": "en"
          },
          "vue-cli-plugin-vuetify-essentials": {},
          "vue-cli-plugin-vuetify-cli": {}
      },
      "router": true,
      "routerHistoryMode": true,
      "vuex": true,
      "configs": {
        "vue": {
          "devServer": {
            "disableHostCheck": true
          }
        }
      }
  }