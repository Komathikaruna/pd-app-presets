{
    "useConfigFiles": true,
    "plugins": {
        "@vue/cli-plugin-babel": {},
        "@vue/cli-plugin-eslint": {
          "config": "standard",
          "lintOn": ["save"]
        },
        "vue-cli-plugin-vuetify": {
          "preset": "configure",
          "replaceComponents": false,
          "useTheme": false,
          "useCustomProperties": false,
          "iconFont": "mdi",
          "installFonts": false,
          "useAlaCarte": true,
          "usePolyfill": true,
          "locale": "en"
        },
        "vue-axios-plugin": {}
      },
      "router": true,
      "routerHistoryMode": true,
      "vuex": true,
      "configs": {
        "vue": {
          "vue-template-compiler" : "*"
        }
      }
  }